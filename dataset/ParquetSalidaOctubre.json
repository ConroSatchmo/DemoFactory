{
	"name": "ParquetSalidaOctubre",
	"properties": {
		"linkedServiceName": {
			"referenceName": "storageconra",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"carpeta": {
				"type": "String"
			},
			"archivo": {
				"type": "String"
			}
		},
		"folder": {
			"name": "PruebaOctubre"
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().carpeta,'/',dataset().archivo)",
					"type": "Expression"
				},
				"folderPath": "AW",
				"fileSystem": "octubre"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}