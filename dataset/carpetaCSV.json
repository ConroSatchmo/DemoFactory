{
	"name": "carpetaCSV",
	"properties": {
		"linkedServiceName": {
			"referenceName": "Storage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"zona": {
				"type": "string"
			},
			"carpeta": {
				"type": "string"
			},
			"archivo": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Parametrizacion"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().archivo",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().zona, '/', dataset().carpeta)",
					"type": "Expression"
				},
				"fileSystem": "ejercitacion"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}