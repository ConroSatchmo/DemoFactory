				"          Mill_Id,",
				"          Order_Number,",
				"          Order_Item,",
				"          Order_Item_Creation_Date,",
				"          Product_Code,",
				"          Requested_Tn_Qty,",
				"          Segment_Id,",
				"          Invoicing_Customer_Id,",
				"          Production_Order_Key,",
				"          COMM_ORDER_ITM_ID,",
